<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>RPMS - Login</title>

    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Nunito:wght@300;400;600;700;800&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="~/rpms/css/bootstrap.css">

    <link rel="stylesheet" href="~/rpms/vendors/iconly/bold.css">

    <link rel="stylesheet" href="~/rpms/vendors/perfect-scrollbar/perfect-scrollbar.css">
    <link rel="stylesheet" href="~/rpms/vendors/bootstrap-icons/bootstrap-icons.css">
    <link rel="stylesheet" href="~/rpms/css/app.css">
    <link href="~/dist/sweetalert2.css" rel="stylesheet" />
    <script src="~/dist/sweetalert2.min.js"></script>

</head>


<body>

    @if (TempData["notification"] != null)
    {
        @Html.Raw(TempData["notification"])
    }

    @RenderBody()

    <!-- sweet alert plugins -->


    <script src="https://code.jquery.com/jquery-1.11.0.min.js"></script>

    <script src="~/rpms/vendors/perfect-scrollbar/perfect-scrollbar.min.js"></script>

    <script src="~/rpms/js/bootstrap.min.js"></script>
    @*<script src="~/rpms/js/bootstrap.bundle.min.js"></script>*@


    <script src="~/rpms/vendors/apexcharts/apexcharts.js"></script>
    @*<script src="~/rpms/js/pages/dashboard.js"></script>*@
    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>



